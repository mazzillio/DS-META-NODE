// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Sale{
  id String @id
  seller_name String 
  visted Decimal
  deals Boolean
  amount Decimal
  date DateTime
  @@map("tb_sales")
}
